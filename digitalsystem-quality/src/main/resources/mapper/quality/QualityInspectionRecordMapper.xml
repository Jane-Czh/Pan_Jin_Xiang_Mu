    <update id="updateQualityInspectionRecord" parameterType="QualityInspectionRecord">
        update Quality_Inspection_Record
        <trim prefix="SET" suffixOverrides=",">
            <if test="yearAndMonth != null">Year_And_Month = #{yearAndMonth},</if>
            <if test="electricCarProblemCount != null">Electric_Car_Problem_Count = #{electricCarProblemCount},</if>
            <if test="k2lessthan5tonProblemCount != null">K2LessThan5Ton_Problem_Count = #{k2lessthan5tonProblemCount},</if>
