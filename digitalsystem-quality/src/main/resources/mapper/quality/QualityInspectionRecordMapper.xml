    <select id="selectQualityInspectionRecordList" parameterType="QualityInspectionRecord" resultMap="QualityInspectionRecordResult">
        <include refid="selectQualityInspectionRecordVo"/>
        <where>
            <if test="yearAndMonth != null "> and Year_And_Month = #{yearAndMonth}</if>
            <if test="electricCarProblemCount != null "> and Electric_Car_Problem_Count = #{electricCarProblemCount}</if>
            <if test="k2lessthan5tonProblemCount != null "> and K2LessThan5Ton_Problem_Count = #{k2lessthan5tonProblemCount}</if>
            <if test="k2largetonnageProblemCount != null "> and K2LargeTonnage_Problem_Count = #{k2largetonnageProblemCount}</if>
